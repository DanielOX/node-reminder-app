<div class="container">
        <div class="row">
                <div class="col s12 m8 offset-m2 l6 offset-l3">
                        <form action="/users/search" method="post">
                            <div class="input-field">
                                    <label for="Search-Bar">Search User</label>
                                    <input id="Search-Bar" type="text" name="search" class="input" />
                                </div>
                        </form>                
                </div>
            <div class="col s12 m8 offset-m2 l6 offset-l3">
                    <% users.forEach((user) => { %>
                <div class="card-panel grey lighten-5 z-depth-1">
                        <div class="row valign-wrapper">
                        <div class="col s2">
                            <img src="/images/avatar.png" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
                        </div>
                        <div class="col s10">
                            <span class="black-text">
                                <h5><%= user.name %></h5>
                                <h6 style="font-size:14px"><%= user.email %></h5>
                                <i style="font-size:12px">                                     
                                        <%= user.info %>
                                </i>                                
                            </span>
                        </div>
                        </div>
                    </div>
                    <% }) %>
                </div>
        </div>
    
    
</div>